<nav aria-label="Page navigation">
  <ul class="pagination">
      {% set previousLink = paginators.previous ? paginators.previous : '#' %}
      {% set prevdisabled = paginators.previous ? '' : 'disabled' %}
    <li class="page-item {{prevdisabled}}">
      <a class="page-link" href="{{currentPage}}{{previousLink}}" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>
    {% for page in paginators.pagers %}
      {% set active = ( page.number == paginators.active ) ? 'active' : '' %}
      <li class="page-item {{active}}">
        <a class="page-link" href="{{currentPage}}{{page.link}}">
          {{page.number}}
        </a>
      </li>
    {% endfor %}
    
      {% set nextLink = paginators.next ? paginators.next : '#' %}
      {% set nextdisabled = paginators.next ? '' : 'disabled' %}
    <li class="page-item {{nextdisabled}}">
      <a class="page-link" href="{{currentPage}}{{nextLink}}" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>